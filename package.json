{
  "name": "@fdosruiz/packetjs",
  "version": "1.0.6",
  "description": "Dependency injection container",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:js && npm run build:ts",
    "build:js": "babel src --out-dir lib --extensions '.ts' --ignore '**/*.test.ts' --source-maps inline",
    "build:ts": "npx tsc --diagnostics --declaration --emitDeclarationOnly --skipLibCheck  --outDir lib src/index",
    "sandbox-ts": "nodemon sandbox/index.ts",
    "sandbox-lib": "nodemon sandbox/lib.js",
    "test": "jest"
  },
  "author": "Francisco Ruiz",
  "repository": {
    "type": "git",
    "url": "https://github.com/fdosruiz/packetjs.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "dependency-injection",
    "dependency-injection-container",
    "di",
    "injection",
    "dependency",
    "container",
    "lazy",
    "lazy-loading",
    "javascript",
    "js",
    "node",
    "nodejs",
    "nodejs",
    "pimple"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "babel-jest": "^28.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "core-js": "^3.23.4",
    "coveralls": "^3.1.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
