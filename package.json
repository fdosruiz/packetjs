{
  "name": "packetjs-di",
  "version": "1.2.8",
  "description": "Lightweight micro dependency injection container",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:js && npm run build:ts",
    "build:js": "babel src --out-dir lib --extensions '.ts' --ignore '**/*.test.ts' --source-maps",
    "build:ts": "npx tsc --diagnostics --declaration --emitDeclarationOnly --skipLibCheck  --outDir lib src/index",
    "lint": "eslint --ext .js,.ts ./",
    "prepare": "husky install",
    "sandbox-lib": "nodemon sandbox/lib.js",
    "sandbox-ts": "nodemon sandbox/index.ts --watch sandbox/common.js --watch sandbox/index.ts",
    "test": "jest"
  },
  "author": "Francisco Ruiz",
  "repository": {
    "type": "git",
    "url": "https://github.com/fdosruiz/packetjs.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "dependency-injection",
    "dependency-injection-container",
    "di",
    "injection",
    "dependency",
    "container",
    "lazy",
    "lazy-loading",
    "javascript",
    "js",
    "node",
    "nodejs",
    "pimple"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.12",
    "@types/uniqid": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-minify": "^0.5.2",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "core-js": "^3.23.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "uniqid": "^5.4.0"
  }
}
