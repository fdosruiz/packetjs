{
  "name": "packetjs-di",
  "version": "1.1.3",
  "description": "Lightweight micro dependency injection container",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:js && npm run build:ts",
    "build:js": "babel src --out-dir lib --extensions '.ts' --ignore '**/*.test.ts' --source-maps",
    "build:ts": "npx tsc --diagnostics --declaration --emitDeclarationOnly --skipLibCheck  --outDir lib src/index",
    "lint": "eslint --ext .js,.ts ./",
    "prepare": "husky install",
    "sandbox-lib": "nodemon sandbox/lib.js",
    "sandbox-ts": "nodemon sandbox/index.ts",
    "test": "jest"
  },
  "author": "Francisco Ruiz",
  "repository": {
    "type": "git",
    "url": "https://github.com/fdosruiz/packetjs.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "dependency-injection",
    "dependency-injection-container",
    "di",
    "injection",
    "dependency",
    "container",
    "lazy",
    "lazy-loading",
    "javascript",
    "js",
    "node",
    "nodejs",
    "nodejs",
    "pimple"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-jest": "^28.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-minify": "^0.5.2",
    "core-js": "^3.23.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
